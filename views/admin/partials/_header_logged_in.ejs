<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>JA - Admin</title>
    <link rel="stylesheet" href="/index.css">
  </head>
  <body>
    <% const navItems = [
      { path: '', label: 'Overview' },
      { path: 'audio', label: 'Audio' },
      { path: 'video', label: 'Video' },
      { path: 'photo', label: 'Photos' },
      { path: 'logout', label: 'Logout' },
    ] %>
    <div class="main-admin-container">
      <section class="side-nav">
        <ul>
          <% navItems.forEach((item, index) => { %>
            <li class="<%= active === item.label ? 'active' : '' %>">
              <a href="/admin/<%= item.path %>">
                <%= item.label %>
              </a>
            </li>
          <% }) %>
        </ul>
      </section>
